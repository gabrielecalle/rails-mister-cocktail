<%= simple_form_for (@cocktail) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name, label: "Cocktail's name", error: "You need to add a cocktail's name"%>

  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.submit "Submit Cocktail", class: "btn btn-primary"%>
<% end %>
